{% if viewAction %}
<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "EmailMessage",
  "description": "{{ viewAction.description }}",
  "potentialAction": {
    "@type": "ViewAction",
    "target": "{{ viewAction.url | raw }}",
    "name": "{{ viewAction.name }}"
  },
  "publisher": {
    "@type": "Organization",
    "name": "{{ viewAction.organization }}",
    "url": "{{ viewAction.organization_url | raw }}"
  }
}
</script>
{% endif %}
<body style='font-family:Helvetica,Verdana,Arial,sans-serif; font-size: 14px;'>
